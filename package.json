{
    "name": "isin-code",
    "displayName": "ISIN Code Lookup",
    "description": "Quickly lookup an ISIN code for basic information.",
    "repository": {
        "type": "git",
        "url": "https://github.com/phacks/vscode-isin-code.git"
    },
    "homepage": "https://github.com/phacks/vscode-isin-code/blob/master/README.md",
    "license": "SEE LICENSE IN LICENSE.txt",
    "galleryBanner": {
        "color": "#302b63",
        "theme": "dark"
    },
    "keywords": [
        "isin",
        "ISIN",
        "lookup",
        "finance",
        "funds",
        "financial"
    ],
    "preview": false,
    "version": "0.1.0",
    "publisher": "phacks",
    "icon": "images/icon-256x256.png",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:plaintext",
        "onLanguage:json",
        "onLanguage:csv"
    ],
    "main": "./out/extension",
    "capabilities": {
        "hoverProvider": "true"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "ISIN Code configuration",
            "properties": {
                "isin-code.OpenFIGIAPIKey": {
                    "type": "string",
                    "default": "",
                    "description": "Your OpenFIGI API Key"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.21.1"
    }
}
